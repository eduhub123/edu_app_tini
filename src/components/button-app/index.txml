<view tiki:if="{{type === 'solid'}}" class="button-app-wrap button-solid {{className}} {{disabled ? 'disable-solid' : ''}}" onTap="_onTap">
  <text number-of-lines="1">{{title}}</text>
</view>
<view tiki:elif="{{type === 'outline'}}" class="button-app-wrap button-outline {{className}} {{disabled ? 'disable-outline' : ''}}" onTap="_onTap">
  <text number-of-lines="1">{{title}}</text>
</view>
